 // 지시사항을 참고하여 solution 함수 안에 코드를 작성하세요. 
function solution(s) {
  var answer = ""
  for(var i=0; i<s.length; i++) {
      if(s[i]==' ') answer = answer + '링디기디기\n'
      else if(s[i]=='.') answer = answer + '딩딩딩\n'
      else answer = answer + '링딩동 '
  }

  return answer;
}


// 실행 혹은 제출을 위한 코드입니다. 지우거나 수정하지 말아주세요.
module.exports = solution;
