 // 지시사항을 참고하여 solution 함수 안에 코드를 작성하세요. 
function solution(str) {
    let sol = ""
    for(let i=0; i<str.length/2; i++) {
        if(str[i]==str[str.length-i-1]) {
            sol+='Same'
        }
        else sol+='Different'

        if(i!=str.length/2-1) sol+='\n'
    }
  return sol;
}


// 실행 혹은 제출을 위한 코드입니다. 지우거나 수정하지 말아주세요.
module.exports = solution;
